*.alicdn.com = server:223.5.5.5 // 阿里 CDN
*.aliyundrive.com = server:223.5.5.5 // 阿里云盘
*.aliyun.* = server:223.5.5.5 // 阿里云
*.aliyuncdn.* = server:223.5.5.5 // 阿里云 CDN

