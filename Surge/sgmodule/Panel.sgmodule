#!name=Panels
#!desc=自用的信息面板
#!system=ios

[Panel]
策略面板 = script-name="节点信息",update-interval=3600
#策略面板 = script-name="解锁检测",update-interval=7200
策略面板 = script-name="DNS",update-interval=600
策略面板 = script-name="运行时间",update-interval=600
策略面板 = script-name="流量统计",update-interval=300
#策略面板 = script-name="网络详情",update-interval=120
策略面板 = script-name="奈飞检测",update-interval=10800
策略面板 = script-name="倒数日",update-interval=86400

[Script]
节点信息 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/stardustXHJie/Hello/main/Surge/JS/IPcheck.js,script-update-interval=0,argument=title=节点信息&icon=externaldrive.connected.to.line.below&color=#9a7ff7
#解锁检测 = type=generic,timeout=30,script-path=https://raw.githubusercontent.com/stardustXHJie/Hello/main/Surge/JS/Stream-All.js,script-update-interval=0,argument=title=解锁检测
DNS = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/stardustXHJie/Hello/main/Surge/JS/flushDNS.js,script-update-interval=0,argument=title=DNS&icon=network&color=#5d84f8
运行时间 = type=generic,timeout=10,script-path= https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/surgepro_reloadprofile.js,script-update-interval=0,argument=title=Surge Pro&icon=crown.fill&color=#f6c970
流量统计 = type=generic,timeout=10,script-path= https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/trafficstatistics.js,script-update-interval=0,argument=title=流量统计&icon=arrow.up.arrow.down.circle&color=#5d84f8
#网络详情 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/net_info.js,script-update-interval=0,argument=title=网络详情&icon=externaldrive.connected.to.line.below&color=#9a7ff7&group=Miaona
奈飞检测 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/stardustXHJie/Hello/main/Surge/JS/nf_check.js,script-update-interval=0,argument=title=𝗡𝗘𝗧𝗙𝗟𝗜𝗫
倒数日 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/stardustXHJie/Hello/main/Surge/JS/Timecard.js,script-update-interval=0,argument=title=倒数日
